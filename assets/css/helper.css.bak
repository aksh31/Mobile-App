*
{
	padding:0;
	margin:0;
	box-sizing:border-box;
	/* font-family: 'Roboto', sans-serif; */
	font-family: 'Lato', sans-serif;
}
.container
{
	width:1100px;
	margin:0 auto;
}
.row, .row:after, .row:before
{
	clear:both;
	display:table;
	width:100%;
}
a:link
{
	text-decoration:none;
	/* display:inline-block; */
}
.col-3, .col-4, .col-5, .col-6, .col-7, .col-8, .col-9, .col-12
{
	float:left;
	padding:0 15px;
}
.col-12
{
	width:100%;
}
.col-9
{
	width:75%;
}
.col-8
{
	width:calc((100% * 8) / 12);
}
.col-7
{
	width:calc((100% * 7) / 12);
}
.col-6
{
	width:50%;
}
.col-5
{
	width:calc((100% * 5) / 12);
}
.col-4
{
	width:calc((100% * 4) / 12);
}
.col-3
{
	width:25%;
}
ul
{
	list-style:none;
}
.text-center
{
	text-align:center;
}
.text-right
{
	text-align:right;
}
.m-auto
{
	margin:0 auto;
}
.mt-40
{
	margin-top: 40px;
}
.mt-15
{
	margin-top: 15px;
}
